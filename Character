
public class Character {

	public static void main(String[] args) {
		System.out.println("생활코딩");
		System.out.println('생');
		//System.out.println('생활코딩');문자형식을 표현하는 기호안에 문자열이 들어갔기 때문에 작동불가
		System.out.println("생");
		//문자 = 1자 문자열 = 1자이상
		//문자는 ''를 사용하고 문자열은""를 사용한다, 문자열은 문자를 포함한다
		System.out.println("생활"+"코딩");
		
		
		
		System.out.println("1"+"1");
		//""안에 있는 숫자는 문자열이 되기 대문에 + 기호를 써도 덧셈이 아니라 11이 출력된다
		System.out.println(1+1);
		
		System.out.println("egoing said \"Welcome programming world\"");
		//""는 문자열을 쓸때 사용하는 기호이다 하지만 "도 문자열로 출력을 원할때에는 기호앞에 \를 붙인다
		// \"를 사용하면 기호가 아닌 문자열로 "를 출력할 수 있게 된다
		
		
		System.out.println("헬로\n월드");
		//줄바꿈을 하고싶을때에는 줄바꿈 할 위치에서 \n을 추가한다
		System.out.println("줄\n바\n꿈\n");
		
		
	}

}
